{
  "name": "node-server-testing",
  "version": "1.0.0",
  "description": "Node Server Testing Challenge",
  "scripts": {
    "test": "cross-env DB_ENV=testing jest --watch --verbose --runInBand",
    "start": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "reset": "npm run rollback && npm run migrate && npm run seed",
    "seed": "knex seed:run"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LambdaSchool/node-server-testing.git"
  },
  "license": "ISC",
  "devDependencies": {
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "knex-cleaner": "^1.3.1",
    "nodemon": "^2.0.13",
    "supertest": "^6.0.1"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "knex": "^0.95.11",
    "prettier": "^2.4.1",
    "sqlite3": "^5.0.2"
  }
}
